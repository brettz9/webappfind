{
    "fileMatches": [
        ["\\.html", "htmltype"],
        ["\\.txt", "txt"],
        ["\\.json", "json"],
        ["\\.xml", "xml"],
        ["\\.css", "csstype"],
        ["\\.csv", "csvtype"],
        ["\\.jpe?g", "jpegtype"],
        ["\\.png", "pngtype"],
        ["\\.gif", "giftype"],
        ["\\.ico", "icotype"],
        ["\\.svg", "svgtype"],
        ["\\.pdf", "pdftype"],
        ["\\.webm", "webmvideotype"],
        ["\\.ogg", "oggaudiotype"],
        ["\\.blockly", "blockly"],
        [".*", "mytype"]
    ],
    "hierarchy": {
        "blockly": ["xml"],
        "mytype": ["json", "js", "txt"],
        "htmltype": ["txt"],
        "csstype": ["txt"],
        "svgtype": ["txt"]
    },
